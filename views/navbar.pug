nav.navbar-expand-lg.navbar-light.bg-light
  // .container

  dev.d-flex.flex-column.bd-highlight
    .d-flex.justify-content-between.pt-2.px-2
      dev
        a.navbar-toggler.border-0
          i.fa.fa-user.mt-3(aria-hidden='true')

        a.navbar-toggler.border-0
          i.fas.fa-shopping-cart.mt-2

      a.navbar-brand.m-0.p-0
        img.mx-2.hideMAX(src='/images/logo01.png', alt='Mawadi', data-purpose='mawadi-brand-logo', height='45')

      button.navbar-toggler.collapsed.border-0(type='button', data-toggle='collapse', data-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
        i.fa.fa-bars(aria-hidden='true')

    .px-2.pb-2.bd-highlight.border-bottom.border-3.navbar-toggler.shadowUnder
      label.sr-only(for='inlineFormInputGroup') Username
      .input-group
        input#inlineFormInputGroup.form-control(type='text', placeholder='Username')
        .input-group-prepend
          .input-group-text
            i.fa.fa-search(aria-hidden='true')

  #navbarSupportedContent.navbar-toggler.collapse.pt-1
    //- .d-flex.flex-column.bd-highlight.mb-3
    .p-2.bd-highlight.border-bottom Internal Lights
    .p-2.bd-highlight.border-bottom exterior Lights
    .p-2.bd-highlight.border-bottom Bulbs
  

    // a.navbar-brand.m-0.p-0
      img.udemy-logo.mx-2(src='https://www.udemy.com/staticx/udemy/images/v6/logo-coral.svg', alt='Udemy', data-purpose='udemy-brand-logo', height='45')
    
  


  // Navigation Bar on Desktop sizr  
  dev
    .col
      .navbar-collapse.collapse.justify-content-around.p-2(style='')
        // .hideMIN
        

        a.navbar-brand.nav-item.m-0.p-0.col-auto
          img.udemy-logo.mx-2.hideMIN(src='/images/logo01.png', alt='Mawadi', data-purpose='udemy-brand-logo', height='45')

        //- ul.navbar-nav.mr-auto
        .col
          label.sr-only(for='inlineFormInputGroup') Username
          .input-group
            input#inlineFormInputGroup.form-control(type='text', placeholder='Username')
            .input-group-prepend
              .input-group-text
                i.fa.fa-search(aria-hidden='true')


        .navbar-nav.mx-4.ml-md-0
          a.nav-item.nav-link.mx-4(href='/')
            i.fa.fa-shopping-cart.mt-1(aria-hidden='true')
          
          .nav-item
            if currentUser
              div
                img.avatar.img-circle.hidden-xs-downborder.border.collapse(src='/images/avatar.png', alt='avatar')
                a.btn.pull-md-right.btn-secondary(href='/logout') Log out
            else  
              a.btn.btn-info.pull-md-right.mr-md-2.collapse(href='/login')
                i.fa.fa-user.mr-2(aria-hidden='true')
                span Login
              //- a.btn.btn-outline-danger.pull-md-right(href='/register') Sign Up
    .col.menu.navbar-collapse.collapse.border-bottom.border-3.shadowUnder
      .col.d-flex.bd-highlight.mx-2
        .p-2.flex-fill.bd-highlight.text-center.tomato.text-white
